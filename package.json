{
  "name": "react-gos",
  "version": "0.1.0",
  "private": true,
  "homepage":"http://mkadyrov.github.io/dashboard",
  "dependencies": {
    "chart.js": "^2.9.3",
    "node-sass": "^4.13.0",
    "react": "^16.12.0",
    "react-chartjs-2": "^2.8.0",
    "react-charts": "^2.0.0-beta.6",
    "react-dom": "^16.12.0",
    "react-image": "^2.2.1",
    "react-native": "^0.61.4",
    "react-native-elements": "^1.2.7",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-scrollbar": "^0.5.6",
    "recharts": "^1.8.5"
  },
  "scripts": {
    "start": "npm-run-all --parallel watch:css start:react",
    "build": "npm-run-all build:css build:react",
    "build:css": "postcss src/styles/tailwind.css -o src/index.css -w",
    "watch:css": "postcss src/styles/tailwind.css -o src/index.css -w",
    "start:react": "react-scripts start",
    "build:react": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.7.2",
    "gh-pages": "^2.1.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "tailwindcss": "^1.1.3"
  }
}
